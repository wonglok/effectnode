{
  "name": "effectnode",
  "version": "1.5.3",
  "description": "effectnode for react",
  "author": "wonglok",
  "license": "MIT",
  "repository": "wonglok/effectnode",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "concurrently \"next dev\"",
    "build": "npm run build-lib; next build",
    "build-lib": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "disabed_predeploy": "cd example && yarn install && yarn run build",
    "disabed_deploy": "gh-pages -d example/build",
    "lt": "ngrok http 3000 --subdomain wonglok --region ap"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.3",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.0.3",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-tailwind": "^0.2.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^2.2.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "microbundle-crl": "^0.13.10",
    "next-offline": "^5.0.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "^3.4.1",
    "tailwindcss": "^3.1.6",
    "three": "^0.139.0",
    "three-stdlib": "^2.12.1",
    "three143": "npm:three@^0.143.0",
    "url-loader": "^4.1.1",
    "zustand": "^4.0.0-rc.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "next": "^12.2.5",
    "nextjs": "^0.0.3"
  }
}
